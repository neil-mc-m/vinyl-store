{% extends 'backend/base.html.twig' %}
{% block content %}
<div class="row">
    <div class="col m6">
        {% for image in images %}
        <div class="card blue-grey darken-1">
            <div class="card-image">
                <img src="/images/uploads/{{ image.image|raw }}" alt="cover_picture_">
            </div>
        </div>
        {% endfor %}
    </div>    
</div>

{% endblock %}
