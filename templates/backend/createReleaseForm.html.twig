{% extends 'backend/base.html.twig' %}
{% set active_page = 'create-release' %}
{% block content %}
    {% set alertTypeAvailable = ['success', 'failure'] %}
    {% for alert in alertTypeAvailable %}
        {% for message in app.session.getFlashBag.get(alert) %}
            <div class="card-panel teal lighten-2">
                {{ message }}
            </div>
        {% endfor %}
    {% endfor %}
    <div class="row">
        <div class="col s12 m12">
            <h5>Create/edit a release</h5>
        </div>
    </div>
    <div class="divider"></div>
    {% include 'backend/partials/releaseForm.html.twig' %}
{% endblock %}