<![endif]-->
    {# the header #}
    <header>
        {% include 'frontend/partials/navbar.html.twig' %}
    </header>
    <main>
        {% block content %} {% endblock %}
    </main>
    <div class="spacer"></div>
    {# the footer #}
    <footer class="page-footer">
        {% include 'frontend/partials/footer.html.twig' %}

    </footer>

    {# javascript functions #}
    <!--Import jQuery before materialize.js-->
    <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.8/js/materialize.js" integrity="sha256-GzgzLo/c1kTNP/m6EvfAaqGLLuvLicxoYs4s6qFRocI=" crossorigin="anonymous"></script>
    <!-- snipcart js -->
    <script src="https://cdn.snipcart.com/scripts/2.0/snipcart.js" id="snipcart"
            data-api-key="OGJiNWViNTItYzFjOC00NjcwLTljYmMtZTc1MDk1ZWZjZWVmNjM2MTc3ODQwOTA5OTg3NjAz" data-autopop="false"></script>
